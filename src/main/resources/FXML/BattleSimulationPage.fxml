<AnchorPane fx:id="pane" prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ntnu.idatt2001.jarandjr.wargames.App.Controllers.BattleSimulationController">
   <VBox fx:id="titleTxtArmiesBattlePage" prefHeight="54.0" prefWidth="540.0" AnchorPane.topAnchor="0.0">
