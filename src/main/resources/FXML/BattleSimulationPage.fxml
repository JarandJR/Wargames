         <Label fx:id="txtTitleArmyTwo" prefHeight="17.0" prefWidth="540.0" text="army two" />
      </children>
   </VBox>
    <VBox fx:id="terrainContainer" layoutY="75.0" prefHeight="580.0" prefWidth="1080.0">
